

/menu = {
    /key : "svg",
    /val : {
        /new : "text",
        /id : "test-text",
        /text : "hi!",
        /x : 50,
        /y : 60,
        /style : {
            /fill : "black"
        },
        # note that in these functions there are no leading slashes, 
        # since it's pure JS at this point
        /onmouseover : "
            this.style.fill = 'red';
        ",
        /onmouseleave : "
            this.style.fill = 'black';
        "
    }

}